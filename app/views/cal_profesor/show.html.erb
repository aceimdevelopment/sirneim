<%- model_class = CalProfesor -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:cal_usuario_ci) %>:</strong></dt>
  <dd><%= @cal_profesor.cal_usuario_ci %></dd>
  <dt><strong><%= model_class.human_attribute_name(:cal_departamento_id) %>:</strong></dt>
  <dd><%= @cal_profesor.cal_departamento_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              cal_profesor_path, class: 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_cal_profesor_path(@cal_profesor), class: 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              cal_profesor_path(@cal_profesor),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              class: 'btn btn-danger' %>
</div>
