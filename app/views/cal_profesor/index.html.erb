<%- model_class = CalProfesor -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:cal_usuario_ci) %></th>
      <th><%= model_class.human_attribute_name(:cal_departamento_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @cal_profesores.each do |cal_profesor| %>
      <tr>
        <td><%= link_to cal_profesor.id, cal_profesor_path(cal_profesor) %></td>
        <td><%= cal_profesor.cal_usuario_ci %></td>
        <td><%= cal_profesor.cal_departamento_id %></td>
        <td><%=l cal_profesor.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_cal_profesor_path(cal_profesor), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      cal_profesor_path(cal_profesor),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_cal_profesor_path,
            :class => 'btn btn-primary' %>
